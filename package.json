{
  "name": "web-qa-usage",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:cypress:open": "cypress open --config-file tests/cypress/cypress.config.ts",
    "test:cypress": "cypress run --config-file tests/cypress/cypress.config.ts",
    "test:testcafe:open": "testcafe firefox tests/testcafe/e2e/*.spec.js --config-file tests/testcafe/testcafe.config.js",
    "test:testcafe": "testcafe firefox:headless tests/testcafe/e2e/*.spec.js --config-file tests/testcafe/testcafe.config.js",
    "test:playwright:open": "playwright test --config tests/playwright/playwright.config.js",
    "test:playwright": "QAU_HEADLESS=true playwright test --config tests/playwright/playwright.config.js"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@playwright/test": "^1.27.1",
    "@vitejs/plugin-vue": "^3.1.0",
    "cypress": "^10.9.0",
    "playwright": "^1.27.1",
    "testcafe": "^2.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  }
}
